{
  "expo": {
    "entryPoint": "./index.js",
    "name": "urbanacess-app",
    "slug": "urbanacess-app",
    "version": "1.0.0",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir que o UrbanAccess acesse sua localização."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "O UrbanAccess precisa acessar suas fotos para reportar problemas de acessibilidade."
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "android": {
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.urbanacessapp"
    },
    "extra": {
      "eas": {
        "projectId": "be1d1fc5-a9ea-4a6e-b905-6d032e03db84"
      }
    }
  }
}
